<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VVVF_Simulator.GUI.MIDIConvert"
        xmlns:MyUserControl="clr-namespace:VVVF_Simulator.GUI.MyUserControl" x:Class="VVVF_Simulator.GUI.MIDIConvert.MIDIConvert_Config"
        mc:Ignorable="d"
        Title="MIDIConvert_Config" Height="450" Width="200" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>


        <Image Source="/GUI/Images/MIDIConvert/B_MultiTask.png" Stretch="Fill" Width="100" Height="100" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Row="1"/>
        <MyUserControl:EnableButton OnClicked="EnableButton_OnClicked" Tag="MultiThread" x:Name="BtnMultiThread" HorizontalAlignment="Center" Grid.Row="1" VerticalAlignment="Top" Width="160" Height="40" Margin="0,129,0,0"/>
        <Label Content="MultiThreading" HorizontalAlignment="Center" Margin="0,100,0,0" Grid.Row="1" VerticalAlignment="Top" FontFamily="M PLUS 2" FontWeight="Bold" Foreground="#FFC3C3C3" FontSize="16"/>

    </Grid>
</Window>
